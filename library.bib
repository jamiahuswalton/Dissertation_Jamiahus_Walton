month = {oct},
pages = {146--158},
publisher = {Elsevier Ltd},
title = {{Long-term implications of sustained wind power growth in the United States: Potential benefits and secondary impacts}},
url = {http://dx.doi.org/10.1016/j.apenergy.2016.06.123 http://linkinghub.elsevier.com/retrieve/pii/S0306261916308984},
volume = {179},
year = {2016}
