keywords = {10,11008 page 1 of,2011,2011 paper no,adaptive training,and education conference,automated scenario generation,dynamic scenarios,feedback,i,industry training,intelligent agents,intelligent tutoring,interservice,itsec,scaffolding,simulation},
number = {11008},
pages = {1--10},
title = {{Understanding the Impact of Intelligent Tutoring Agents on Real-Time Training Simulations}},
year = {2011}
}
