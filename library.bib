journal = {Human Factors: The Journal of the Human Factors and Ergonomics Society},
number = {2},
pages = {234--245},
pmid = {20942253},
title = {{An Empirical Analysis of Team Coordination Behaviors and Action Planning With Application to Human-Robot Teaming}},
