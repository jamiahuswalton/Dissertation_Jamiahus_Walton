keywords = {80 and over,Activities of Daily Living,Adult,Age Factors,Aged,Environment,Executive Function,Female,Humans,Male,Middle Aged,Neuropsychological Tests,Stroke,Stroke: diagnosis,User-Computer Interface,Young Adult},
month = {aug},
number = {4},
pages = {583--602},
pmid = {19058093},
title = {{Validation of the Virtual MET as an assessment tool for executive functions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19058093},
volume = {19},
