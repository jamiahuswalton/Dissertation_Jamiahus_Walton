@article{Hockey2007,
abstract = {Objective: The study examined the adaptability of different types of process control training across changes in task and environmental stress. Background: The literature on training leads us to expect greater flexibility for system-based training, as opposed to procedure-based training. However, the stress literature suggests that knowledge-based strategies (making use of executive control) may be more vulnerable under stress conditions. Method: Two groups were given 6 hr of training on the Cabin Air Management System (CAMS), a complex, multilevel, PC-based process control task, emphasizing either system knowledge or use of procedures. They were then required to carry out the task for 3 hr (with noise during the middle 1 hr) across a range of both familiar and unfamiliar fault scenarios. Results: For the primary control task, the system-trained group performed better, especially for less familiar and complex faults. However, for lower priority tasks requiring executive control, procedure-trained operators performed better and were less impaired by noise. Conclusion: System training was more effective for managing unexpected task events, whereas procedural training was better under noise. The results are interpreted in terms of the rationale for instructing operators in the range of strategies required for effective process skills in complex work environments. Application: Training methodologies for safety critical applications should aim to develop skill in the use of both procedural and system knowledge strategies. Operators should be trained in the most effective deployment of these strategies during unfamiliar task events and environmental stress and given stress exposure training.},
annote = {doi: 10.1518/001872007779598000},
author = {Hockey, G Robert J and Sauer, J{\"{u}}rgen and Wastell, David G},
doi = {10.1518/001872007779598000},
file = {:C$\backslash$:/Users/jamia/Box/Mendeley Desktop/Hockey, Sauer, Wastell - 2007 - Adaptability of Training in Simulated Process Control Knowledge- Versus Rule-Based Guidance Under Task C.pdf:pdf},
issn = {0018-7208},
journal = {Human Factors},
month = {feb},
number = {1},
pages = {158--174},
