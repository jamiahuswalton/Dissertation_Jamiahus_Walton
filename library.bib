journal = {JEDM - Journal of Educational Data Mining},
number = {1},
pages = {3--27},
title = {{Statistical Modeling of Student Performance to Improve Chinese Dictation Skills with an Intelligent Tutor}},
url = {http://www.educationaldatamining.org/JEDM/index.php/JEDM/article/view/40},
volume = {6},
