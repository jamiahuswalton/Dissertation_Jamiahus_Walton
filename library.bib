keywords = {*Education,*Task Analysis,Behavioral Assessment,Educational Objectives,Job Analysis,Learning,Methodology},
pages = {xiv, 408--xiv, 408},
publisher = {Praeger Publishers},
title = {{Handbook of task analysis procedures.}},
year = {1989}
}
